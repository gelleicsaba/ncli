{
    "typescript": true,
    "empty-exports": true,
    "beautify": true,
    "appname": "app",
    "structure": {
        "src": "src",
        "routes": "src/routes",
        "services": "src/services",
        "server": "src/server.ts"
    },
    "server-options": {
        "host": "localhost",
        "port": 3000,
        "enable-cors": true,
        "allow-sources": "*",
        "allow-methods": "GET, POST, PUT, DELETE, PATCH",
        "allow-headers": "X-Requested-With"
    },
    "file-options": {
        "router": "%",
        "service": "%.service"
    },
    "var-options": {
        "router": "%Router",
        "service": "%Service"
    },
    "router-imports": [
        "const express = require('express')",
        "const { connect } = require('../services/connect')"
    ],
    "service-imports": [
        "const { MongoClient } = require('mongodb')"
    ],
    "rest-options": {
        "GET": true,
        "POST": true,
        "PUT": true,
        "DELETE": true,
        "PATCH": false
    },
    "route-params": {
        "async-return": true,
        "request": "req",
        "response": "rsp",
        "typescript-options": {
            "request-type": "any",
            "response-type": "any",
            "nullable": true
        },
        "extra-option": "express.json ({type: '*/*'})"
    },
    "route-body": {
        "start": [
            "const data = req.body"
        ],
        "end": [
            "rsp.json ({'success': true})",
            "rsp.end"
        ],
        "double-quotes": true
    },
    "test-options": {
        "url": "http://localhost:3000",
        "output-dir": "tester",
        "pretty-json": true,
        "input-comments": true,
        "title": "My project api tester"
    }
}